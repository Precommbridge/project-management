// CPP program to print Happy Birthday
#include<bits/stdc++.h>
using namespace std;
  
int main()
{
    // Print first row
    char ch = '@';
    for(int i=1; i<=34; i++)
    {
        if (i==5||i==7||i==10||i==11||i==14||i==15||
           i==16||i==18||i==19||i==20||i==22||i==24)
            cout << ch ;
        else cout << " " ;
  
    }
